
html 
    lang en
    head 
        meta 
            charset UTF-8
        link 
            rel icon
            type image/svg+xml
            href favicon.ico
        meta 
            name viewport
            content width=device-width, initial-scale=1.0
        @title Codex - Your AI
        link 
            rel stylesheet
            href style.css
        style
          $include main.css
    body 
        div 
            id app
            div 
                id chat_container
            form 
                textarea 
                    name prompt
                    rows 1
                    cols 1
                    placeholder Ask codex...
                button 
                    type submit
                    img 
                        src data:image/svg+xml;base64,
                        alt send
            $$ js script.js
            script
              module
                kind react
                $include main.js